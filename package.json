{
  "name": "bomber",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.5",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "framer-motion": "^10.12.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "vite": "^4.2.0"
  },
  "optimization": {
    "splitChunks": {
      "chunks": "all"
    },
    "moduleIds": "deterministic",
    "runtimeChunk": "single",
    "minimize": true,
    "usedExports": true,
    "sideEffects": true,
    "concatenateModules": true,
    "nodeEnv": "production",
    "mangleExports": true,
    "mergeDuplicateChunks": true,
    "removeEmptyChunks": true,
    "flagIncludedChunks": true,
    "occurrenceOrder": true,
    "providedExports": true,
    "emitOnErrors": true,
    "checkWasmTypes": true,
    "realContentHash": true,
    "minimizer": [
      {
        "options": {
          "exclude": [
            "**/node_modules/**"
          ]
        },
        "plugin": {}
      }
    ],
    "include": [
      "react",
      "react-dom"
    ]
  }
  
}
